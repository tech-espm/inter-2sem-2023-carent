<div class="section-search">
	<div class="container">
		<div class="search-box-banner">
			<form action="listing-grid.html">
				<div class="row">
					<div class="col">
						<div class="form-group">
							<label for="pickup-location">Pickup Location</label>
							<input type="text" id="pickup-location" class="form-control"
								placeholder="Enter City, Airport, or Address">
						</div>
					</div>
					<div class="col">
						<div class="form-group">
							<label for="pickup-date">Pickup Date</label>
							<input type="date" id="pickup-date" class="form-control datetimepicker"
								placeholder="04/11/2023">
						</div>
					</div>
					<div class="col">
						<div class="form-group">
							<label for="pickup-time">Time</label>
							<input type="time" id="pickup-time" class="form-control timepicker" placeholder="11:00 AM">
						</div>
					</div>
					<div class="col">
						<div class="form-group">
							<label for="return-date">Return Date</label>
							<input type="date" id="return-date" class="form-control datetimepicker"
								placeholder="04/11/2023">
						</div>
					</div>
					<div class="col">
						<div class="form-group">
							<label for="return-time">Return Time</label>
							<input type="time" id="return-time" class="form-control timepicker" placeholder="11:00 AM">
						</div>
					</div>
					<div class="col">
						<div class="form-group">
							<label for="pesquisar">&nbsp</label>
							<button class="btnpl search-button" type="submit">
								<i class="fa fa-search" aria-hidden="true"></i> Search
							</button>
						</div>
					</div>
				</div>
			</form>
		</div>
	</div>
</div>


